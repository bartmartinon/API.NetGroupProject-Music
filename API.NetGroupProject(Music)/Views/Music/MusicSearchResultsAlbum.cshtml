@model MusicSearch

@{
    List<int> uniqueAlbumIDs = new List<int>();
}
@foreach (Datum d in Model.data)
{
    if (!uniqueAlbumIDs.Contains(d.album.id))
    {
        <p><b>Album ID:</b> @d.album.id</p>
        <p><b>Title:</b> @d.album.title</p>
        <img src="@d.album.cover" alt="@d.album.cover" />
        <p><b>Track List:</b> <a asp-controller="Music" asp-action="MusicSearch" asp-route-id="@d.album.id">@d.album.tracklist</a></p>
        <hr />
        uniqueAlbumIDs.Add(d.album.id);
    }
  @*  <a asp-controller="Music" asp-action="GetAlbumDetail" asp-route-id="@d.album.id">*@
        }
        <p>Length of Datum data: @Model.data.Length </p>
        <p>Unique Albums in data: @uniqueAlbumIDs.Count </p>
        <p>
            @if (Model.prev != null)
            {
                <a href="@Model.prev">&larr; Prev</a>
            }
            |
            @if (Model.next != null)
            {
                <a href="@Model.next">Next &rarr;</a>
            }
        </p>
